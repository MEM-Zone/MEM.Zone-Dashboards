    <DataSet Name="UpdateCategoryOther">
      <Query>
        <DataSourceName>CMSQLDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Locale">
            <Value>=Parameters!Locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Get 'Other' Update Classification Category */

/* Variable declaration */
DECLARE @LCID AS INT = dbo.fn_LShortNameToLCID(@Locale);

SELECT DISTINCT CategoryInstanceName
FROM fn_CICategoryInfo(@LCID)
WHERE CategoryTypeName = N'UpdateClassification'
    AND CategoryInstanceName NOT IN (N'Critical Updates', N'Security Updates', N'Feature Packs')
