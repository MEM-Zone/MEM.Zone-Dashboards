                </Style>
              </Textbox>
              <Textbox Name="TXT_CriticalAlerts_Value">
                <KeepTogether>true</KeepTogether>
                <Paragraphs>
                  <Paragraph>
                    <TextRuns>
                      <TextRun>
                        <Value>=First(Fields!CriticalAlertCount.Value, "CriticalAlertCount")</Value>
                        <ActionInfo>
                          <Actions>
                            <Action>
                              <Drillthrough>
                                <ReportName>=Globals!ReportFolder &amp; "/AL Alerts"</ReportName>
                                <Parameters>
                                  <Parameter Name="AlertFeatureArea">
                                    <Value>4</Value>
                                  </Parameter>
                                  <Parameter Name="AlertState">
                                    <Value>0</Value>
                                  </Parameter>
