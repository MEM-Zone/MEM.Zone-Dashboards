    <DataSet Name="TotalDevicesInfo">
      <Query>
        <DataSourceName>CMSQLDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Total Devices DatasetInfo */
DECLARE @ClientDevices         AS INT = (
    SELECT COUNT(ResourceID)
    FROM fn_rbac_ClientCollectionMembers(@UserSIDs) AS ClientCollectionMembers
    WHERE ClientCollectionMembers.CollectionID = @CollectionID
)
DECLARE @TotalDevices          AS INT = (
    SELECT COUNT(ResourceID)
    FROM fn_rbac_FullCollectionMembership(@UserSIDs) AS CollectionMembership
    WHERE CollectionMembership.CollectionID = @CollectionID
        AND CollectionMembership.ResourceType = 5 --Select devices only
)
SELECT
    ClientDevices  = @ClientDevices
    , TotalDevices = @TotalDevices</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ClientDevices">
          <DataField>ClientDevices</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalDevices">
          <DataField>TotalDevices</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AdvertisementInfo">
      <Query>
        <DataSourceName>CMSQLDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Get Advertisement Info for Collection */
SELECT
	Advertisement.PackageID
	, Advertisement.CollectionName
	, Advertisement.AdvertisementID
	, DisplayName = Advertisement.PackageManufacturer
	+ ' ' + Advertisement.PackageName
	+ ' v' + Advertisement.PackageVersion
	+ ' (' + Advertisement.ProgramName + ')'
FROM v_AdvertisementInfo AS Advertisement
WHERE Advertisement.CollectionID = @CollectionID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PackageID">
          <DataField>PackageID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CollectionName">
