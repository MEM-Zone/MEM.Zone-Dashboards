    <DataSet Name="CollectionInfo">
      <Query>
        <DataSourceName>CMSQLDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Get Device Collections */
SELECT
    CollectionID,
    Name
FROM dbo.fn_rbac_Collection(@UserSIDs)
WHERE CollectionType = 2
ORDER BY Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ComplianceDetails">
      <Query>
        <DataSourceName>CMSQLDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Testing variables !! Need to be commented for Production !! */
--DECLARE @UserSIDs          AS NVARCHAR(10) = 'SMS0001';
--DECLARE @CollectionID      AS NVARCHAR(10) = 'SMS0001';

/* Get compliance data data */
;
WITH Bitlocker_CTE AS (
SELECT
    EncodedDeviceName         = MBAMPolicy.EncodedComputerName0
    , Domain                  = ComputerSystem.Domain0
    , UserName                = SystemValid.User_Domain0 + N'\' + SystemValid.User_Name0
    , ComplianceStatus        = CIComplianceStatus.ComplianceState
    , UploadedKeys            = COUNT(RecoveryCoreKeys.RecoveryKey)                   OVER(PARTITION BY MBAMPolicy.EncodedComputerName0)
    , DisclosedKeys           = COUNT(IIF(RecoveryCoreKeys.Disclosed = 1, '*', NULL)) OVER(PARTITION BY MBAMPolicy.EncodedComputerName0)
    , DiskVolumes		      = (
            DENSE_RANK() OVER(PARTITION BY MBAMPolicy.EncodedComputerName0 ORDER BY BitlockerDetails.MbamPersistentVolumeId0)
            +
            DENSE_RANK() OVER(PARTITION BY MBAMPolicy.EncodedComputerName0 ORDER BY BitlockerDetails.MbamPersistentVolumeId0 DESC)
            - 1
    )
    , Exemption               = ( -- ComplianceState: 1 = Compliant, 2 = NonCompliant
        CASE
            WHEN MBAMPolicy.MBAMPolicyEnforced0 IS NULL                                        THEN -1 -- When outer join returns null: N/A
            WHEN CIComplianceStatus.ComplianceState = 2 AND MBAMPolicy.MBAMPolicyEnforced0 = 3 THEN 1  -- Temporary user exempt
            WHEN CIComplianceStatus.ComplianceState = 1 AND MBAMPolicy.MBAMPolicyEnforced0 = 2 THEN 2  -- User exempt
            ELSE 0 -- Not Exempted
        END
    )
    , ComplianceStatusDetails = ( -- ComplianceState: 1 = Compliant, 2 = NonCompliant
        CASE
            WHEN CIComplianceStatus.ComplianceState = 1 AND MBAMPolicy.MBAMPolicyEnforced0           = 0 THEN 50                           -- Policy not enforced
            WHEN CIComplianceStatus.ComplianceState = 1 AND MBAMPolicy.MBAMPolicyEnforced0           = 1 THEN 0                            -- No Error
            WHEN CIComplianceStatus.ComplianceState = 2 AND MBAMPolicy.MBAMMachineError0 IS NOT NULL     THEN MBAMPolicy.MBAMMachineError0 -- MBAM agent error status
            ELSE -1 -- No Details or Errors
        END
    )
    , LastPolicyEvaluation    = CONVERT(NVARCHAR(19), MAX(AssignmentStatus.LastEvaluationMessageTime), 120)
    , RN                      = ROW_NUMBER() OVER(PARTITION BY MBAMPolicy.EncodedComputerName0 ORDER BY MAX(AssignmentStatus.LastEvaluationMessageTime) DESC)
FROM fn_rbac_FullCollectionMembership(@UserSIDs) AS FullCollectionMembership
    INNER JOIN fn_rbac_R_System_Valid(@UserSIDs) AS SystemValid ON SystemValid.ResourceID = FullCollectionMembership.ResourceID
    INNER JOIN fn_rbac_GS_COMPUTER_SYSTEM(@UserSIDs) AS ComputerSystem ON ComputerSystem.ResourceID = SystemValid.ResourceID
    INNER JOIN v_SMSCICurrentComplianceStatus AS CIComplianceStatus ON CIComplianceStatus.ItemKey = SystemValid.ResourceID
    INNER JOIN fn_rbac_ConfigurationItems(@UserSIDs) AS ConfigurationItems ON ConfigurationItems.ModelID = CIComplianceStatus.ModelID
    LEFT OUTER JOIN fn_rbac_GS_MBAM_POLICY(@UserSIDs) AS MBAMPolicy ON MBAMPolicy.ResourceID = FullCollectionMembership.ResourceID
    LEFT OUTER JOIN fn_rbac_CIAssignmentToCI(@UserSIDs) AS AssignmentCI ON AssignmentCI.CI_ID  = ConfigurationItems.CI_ID
    LEFT OUTER JOIN fn_rbac_CIAssignmentStatus(@UserSIDs) AS AssignmentStatus ON AssignmentStatus.AssignmentID = AssignmentCI.AssignmentID
        AND AssignmentStatus.ResourceID = SystemValid.ResourceID
    LEFT OUTER JOIN fn_rbac_GS_BITLOCKER_DETAILS(@UserSIDs) AS BitlockerDetails ON BitlockerDetails.ResourceID = SystemValid.ResourceID
        AND BitlockerDetails.BitlockerPersistentVolumeId0 != '' -- Remove empty VolumeIDs to prevent errors in RecoveryCoreVolumes Join
        AND BitlockerDetails.MbamVolumeType0 IN (1, 2)          -- 1 = OS, 2 = Fixed Data Volumes
    LEFT OUTER JOIN RecoveryAndHardwareCore_Volumes AS RecoveryCoreVolumes ON RecoveryCoreVolumes.VolumeGuid = (
        SELECT SUBSTRING(BitlockerDetails.BitlockerPersistentVolumeId0, 2, LEN(BitlockerDetails.BitlockerPersistentVolumeId0) -2)
    )                                                           -- Remove '{}' from VolumeID
    LEFT OUTER JOIN RecoveryAndHardwareCore_Keys AS RecoveryCoreKeys ON RecoveryCoreKeys.VolumeId = RecoveryCoreVolumes.Id
WHERE MBAMPolicy.EncodedComputerName0 IS NOT NULL
    AND FullCollectionMembership.IsClient = 1
    AND FullCollectionMembership.CollectionID = @CollectionID
    AND ConfigurationItems.CI_ID IN (
        SELECT BLMCollectionID.CI_ID FROM v_BLM_CI_ID_AND_COLL_ID BLMCollectionID WHERE BLMCollectionID.CollectionID = @CollectionID
    )
GROUP BY
    MBAMPolicy.EncodedComputerName0
    , SystemValid.User_Name0
    , SystemValid.User_Domain0
    , ComputerSystem.Domain0
    , CIComplianceStatus.ComplianceState
    , RecoveryCoreKeys.RecoveryKey
    , RecoveryCoreKeys.Disclosed
    , BitlockerDetails.MbamPersistentVolumeId0
    , MBAMPolicy.MBAMPolicyEnforced0
    , MBAMMachineError0
)
SELECT
    EncodedDeviceName
    , Domain
    , UserName
    , ComplianceStatus
    , UploadedKeys
    , DisclosedKeys
    , DiskVolumes
    , UploadedAllKeys = IIF(UploadedKeys - DisclosedKeys = DiskVolumes, 'Yes', NULL)
    , Exemption
    , ComplianceStatusDetails
    , LastPolicyEvaluation
FROM Bitlocker_CTE AS Bitlocker
WHERE Bitlocker.RN = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
