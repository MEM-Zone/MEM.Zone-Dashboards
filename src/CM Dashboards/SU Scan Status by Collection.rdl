          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="WUAVersion">
          <DataField>WUAVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastScanTime">
          <DataField>LastScanTime</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastScanPackageLocation">
          <DataField>LastScanPackageLocation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastScanPackageVersion">
          <DataField>LastScanPackageVersion</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SyncCatalogVersion">
          <DataField>SyncCatalogVersion</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CatalogVersionsBehind">
          <DataField>CatalogVersionsBehind</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LastScanError">
          <DataField>LastScanError</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReportInfo">
      <Query>
