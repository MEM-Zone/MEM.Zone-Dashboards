          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Get Update Groups */

/* Variable declaration */
DECLARE @LCID AS INT = dbo.fn_LShortNameToLCID (@Locale);

SELECT DISTINCT
    CI_ID
    , Title
FROM fn_rbac_AuthListInfo(@LCID, @UserSIDs)
ORDER BY Title</CommandText>
